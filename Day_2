A Z"""
points = 0
part2_points = 0
P1_ROCK = 1
P1_PAPER = 2
P1_SCIS = 3
P1_WON = 6
P1_DRAW = 3
P1_LOSE = 0

print("--- Day 2: Rock Paper Scissors ---\n\nOponnent inputs: 'A for Rock, B for Paper, and C for Scissors' \n player inputs: 'X for Rock, Y for Paper, and Z for Scissors'. \n A beats Z, A loses Y \n B beats X, loses Z \n C beats Y, loses X")
print("""Points are calculated based on (+ 1 for Rock, + 2 for Paper, and + 3 for Scissors) plus the score for the outcome of the round (0 if you lost, 3 if the round was a draw, and 6 if you won)""")
for line in data.split('\n'):
    input = line.split(' ')
    p2_move = input[0]
    p1_move = input[1]
    ex_result = input[1]
    # bonus = lambda p1_move : P1_ROCK if p1_move == 'X' else( P1_PAPER if p1_move == 'Y' else P1_SCIS)
    bonus = 0
    if p1_move == 'X':
        bonus = P1_ROCK
    elif p1_move == 'Y':
        bonus = P1_PAPER
    else:
        bonus = P1_SCIS

    #P2_Rock
    if p2_move == 'A' and p1_move == 'X':
        points  +=  (P1_DRAW + bonus)
    if p2_move == 'A' and p1_move == 'Y':
        points  +=  (P1_WON + bonus)
    if p2_move == 'A' and p1_move == 'Z':
        points  +=  (P1_LOSE + bonus)
    #P2_Paper    
    if p2_move == 'B' and p1_move == 'Y':
        points  +=  (P1_DRAW + bonus)
    if p2_move == 'B' and p1_move == 'Z':
        points  +=  (P1_WON + bonus)
    if p2_move == 'B' and p1_move == 'X':
        points  +=  (P1_LOSE + bonus)
    #P2_Scissors
    if p2_move == 'C' and p1_move == 'Z':
        points  +=  (P1_DRAW + bonus)
    if p2_move == 'C' and p1_move == 'X':
        points  +=  (P1_WON + bonus)
    if p2_move == 'C' and p1_move == 'Y':
        points +=  (P1_LOSE + bonus)
        
        #p2_P2_Rock
    if p2_move == 'A' and p1_move == 'X':
        part2_points += (P1_LOSE + P1_SCIS)
    if p2_move == 'A' and p1_move == 'Y':
        part2_points += (P1_DRAW + P1_ROCK)
    if p2_move == 'A' and p1_move == 'Z':
        part2_points += (P1_WON + P1_PAPER)
   
    #p2_P2_Paper    
    if p2_move == 'B' and p1_move == 'X':
        part2_points += (P1_LOSE + P1_ROCK)
    if p2_move == 'B' and p1_move == 'Y':
        part2_points += (P1_DRAW + P1_PAPER)
    if p2_move == 'B' and p1_move == 'Z':
        part2_points += (P1_WON + P1_SCIS)
  
    #p2_P2_Scissors
    if p2_move == 'C' and p1_move == 'X':
        part2_points += (P1_LOSE + P1_PAPER)
    if p2_move == 'C' and p1_move == 'Y':
        part2_points += (P1_DRAW + P1_SCIS)
    if p2_move == 'C' and p1_move == 'Z':
        part2_points += (P1_WON + P1_ROCK)
        
print(f'Points obtained via 1st strategy guide: {points}')

print('\n\nPART 2\n')
#while normally I would redo the code above...
print("""Actually mine input  X means you need to lose,\n Y means you need to end the round in a draw,\n and Z means you need to win\n""")

print(f'Points obtained via 2nd strategy guide: {part2_points} .')

