points = 0
prio = list()
item_template = string.ascii_letters
#build prio list quickly
for count, item in enumerate(item_template):
    prio.append((item,count + 1))


for rucksack in data.split('\n'):
    #split each bag in two, compare both HALVES
    comp1 = slice(0, len(rucksack)//2)
    comp2 = slice(len(rucksack)//2, len(rucksack))
    #convert to set to get non duplicated items
    list_comp1 = list(rucksack[comp1])
    # set_comp1 = set(rucksack[comp1])
    list_comp2 = list(rucksack[comp2])
    for items in list_comp1:
        if items in list_comp2:
            for p in prio:
                if p[0] == item:
                    points = points + p[1]
    # set_comp2 = set(rucksack[comp2])
    # #cleanup
    # for item in set_comp1:
    #     list_comp1.remove(item)
    # for item in set_comp2:
    #     list_comp2.remove(item)
    # print(list_comp1)
    # print(list_comp2)
    
#     list_rucksack = list(rucksack)
#     set_rucksack = set(rucksack)
#     # print(f'list:{list_rucksack}\n set {set_rucksack}')
#     for item in set_rucksack:
#         list_rucksack.remove(item)
#     for item in set(list_rucksack):
#         for p in prio:
#             if p[0] == item:
#                 points = points + p[1]
#                 print(p[0], item, p[1], points)
# #print(prio)
print(points)

